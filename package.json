{
  "name": "vuepress-theme-wind",
  "version": "1.0.0",
  "description": "A modern VuePress theme with Tailwind CSS 4 and Fadgram UI components",
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib",
    "templates"
  ],
  "scripts": {
    "dev": "vuepress dev docs",
    "build": "vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs-locales:dev": "vuepress dev docs-locales",
    "docs-locales:build": "vuepress build docs-locales",
    "format": "prettier --write .",
    "clean": "vuepress-vite dev docs --clean-cache",
    "clean-dev": "vuepress dev docs --clean-cache",
    "update-package": "pnpm dlx vp-update",
    "theme:build": "tsc -b tsconfig.build.json",
    "theme:clean": "rimraf lib *.tsbuildinfo"
  },
  "keywords": [
    "vuepress-theme",
    "vuepress",
    "fadgram",
    "vue",
    "tailwindcss",
    "wind",
    "vuepress-tailwindcss",
    "vuepress-tailwindcss4"
  ],
  "author": {
    "name": "Talal Almrka",
    "email": "talalminfo@gmail.com"
  },
  "license": "ISC",
